        round @CL_lg5 "$ID_LEAGUE_PHASE_MATCH_5" type Round1 teams 36 no_flags without_away_goal matchdays CL_LG5 bonus [0,2100000,700000,0] subs SUBS { get(@prev, 145, 36); }
